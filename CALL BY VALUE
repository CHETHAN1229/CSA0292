#include <stdio.h>


void swap(int a, int b) {
    int temp;
    temp = a;
    a = b;
    b = temp;

    printf("Inside swap function:\n");
    printf("a = %d, b = %d\n", a, b);
}

int main() {
    int x = 5, y = 10;

    printf("Before swap in main function:\n");
    printf("x = %d, y = %d\n", x, y);

    
    swap(x, y);

    printf("After swap in main function:\n");
    printf("x = %d, y = %d\n", x, y);

    return 0;
}
OUTPUT 
Before swap in main function:
x = 5, y = 10
Inside swap function:
a = 10, b = 5
After swap in main function:
x = 5, y = 10
